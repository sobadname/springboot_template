		<form method="post" class="form-horizontal" th:action="@{/dsyg/item/page/update}" action="/dsyg/item/page/update" role="form">
		<input type="hidden" name="pageAndPosition" id="pageAndPosition">
		<div class="modal inmodal" id="itemModal" tabindex="-1"
			role="dialog" aria-hidden="true">
			<div class="modal-dialog modal-sm">
				<div class="modal-content animated flipInY">
					<div class="modal-header">
						<button type="button" class="close" data-dismiss="modal">
							<span aria-hidden="true">&times;</span><span class="sr-only">关闭</span>
						</button>
						<h4 class="modal-title">上架商品</h4>
						<small class="font-bold">选择想要上架的商品.</small>
					</div>
					<div class="modal-body">
					<select data-placeholder="选择商品..." name="selectItem" id="selectItem" class="chosen-select"  tabindex="2">
	                <option value="0">选择商品...</option>
	                <option th:each="item,st : ${itemList}"
							th:value="${item.id}"
							th:text="${item.name}" value="1">唯品会</option>
	                </select>
					</div>
					<div class="modal-footer">
						<button type="button" class="btn btn-white" data-dismiss="modal">关闭</button>
						<button type="submit" class="btn btn-primary">提交</button>
					</div>
				</div>
			</div>
		</div>
		</form>
